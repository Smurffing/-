package com.example.weather
import com.example.weather.MainActivity
class WeatherResponse {
    data class WeatherResponse(
        val name: String,
        val main: MainData,
        val weather: List<WeatherData>
    )

    data class MainData(
        val temp: Double,
        val humidity: Int
    )

    data class WeatherData(
        val description: String
    )
}
